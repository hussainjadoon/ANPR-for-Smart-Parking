<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sherybutt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Feed_Back"></activity>
        <activity android:name=".User_details" />
        <activity android:name=".New_Booking" />
        <activity android:name=".Forgot_Password" />

        <meta-data
            android:name="firebase_analytics_collection_enabled"
            android:value="true" />

        <activity android:name=".Dashboard" />
        <activity android:name=".Sign_up_page" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>